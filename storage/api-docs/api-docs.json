{
    "openapi": "3.0.0",
    "info": {
        "title": "Number Categorization API",
        "description": "API for categorizing numbers based on length and digit patterns",
        "contact": {
            "email": "admin@example.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api",
            "description": "Local API server"
        }
    ],
    "paths": {
        "/categorize": {
            "post": {
                "tags": [
                    "Number Categorization"
                ],
                "summary": "Categorize a number based on its length and digit patterns",
                "operationId": "8e92ba300cf7b51d05361c765f1151e3",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "number",
                                    "length"
                                ],
                                "properties": {
                                    "number": {
                                        "type": "string",
                                        "example": "5543022"
                                    },
                                    "length": {
                                        "type": "integer",
                                        "example": 7
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "category": {
                                            "type": "string",
                                            "example": "Gold"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Length must be between 3 and 9"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Number Categorization",
            "description": "Number Categorization"
        }
    ]
}